<?xml version="1.0"?>
<jbi
    version="1.0"
    xmlns="http://java.sun.com/xml/ns/jbi"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:config="http://www.sun.com/jbi/descriptor/configuration"    
    xmlns:identification="http://www.sun.com/jbi/descriptor/identification"
    xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
<!-- identification information about this shared library-->
    <shared-library>
        <identification>
            <name>sun-common-library</name>
            <description>This library provides utility classes for automatic security and transaction propogation.</description>
        </identification>
        <shared-library-class-path>
            <path-element>lib/common-util.jar</path-element>
         </shared-library-class-path>
    </shared-library>
</jbi>
    