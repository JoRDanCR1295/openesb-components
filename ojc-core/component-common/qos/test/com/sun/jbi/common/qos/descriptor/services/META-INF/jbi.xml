<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jbi version="1.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://java.sun.com/xml/ns/jbi"
        xsi:schemaLocation="http://java.sun.com/xml/ns/jbi jbi.xsd"
        xmlns:ns0="http://j2ee.netbeans.org/wsdl/CandidateEvaluation">
    <services binding-component="false">
        <provides interface-name="ns0:CandidateEvaluationPortType" 
        		  service-name="ns0:CandidateEvaluator" 
        		  endpoint-name="evaluator">
        	<application-config xmlns="http://www.sun.com/jbi/descriptor/configuration" 
        		name="myProviderConfig"/>
       	</provides>
        <consumes interface-name="ns0:CandidateEvaluationPortType" 
        		  service-name="ns0:CandidateEvaluator" 
        		  endpoint-name="evaluator">
        	<application-config xmlns="http://www.sun.com/jbi/descriptor/configuration" 
        		name="myConsumerConfig"/>
        </consumes>
    </services>
 </jbi>
