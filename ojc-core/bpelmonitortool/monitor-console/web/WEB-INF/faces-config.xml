<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <application>
        <view-handler>
            com.sun.rave.web.ui.appbase.faces.ViewHandlerImpl
        </view-handler>
    </application>
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>bpelmonitor.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>bpelmonitor.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>bpelmonitor.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
            <!-- Basic Tree example bean-->
    <managed-bean>
        <description>
            Backing bean for Service Assemblies and Business Processes
        </description>
        <managed-bean-name>BPTreeList</managed-bean-name>
        <managed-bean-class>bpelmonitor.BPList</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>BPInstancesList</managed-bean-name>
        <managed-bean-class>bpelmonitor.InstancesList</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>SVG</managed-bean-name>
        <managed-bean-class>bpelmonitor.svg.SVG</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>JBIRuntimeService</managed-bean-name>
        <managed-bean-class>bpelmonitor.jbiruntime.JBIRuntime</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Dashboard</managed-bean-name>
        <managed-bean-class>bpelmonitor.Dashboard</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>LeftNavigation</managed-bean-name>
        <managed-bean-class>bpelmonitor.LeftNavigation</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ProcessMonitor</managed-bean-name>
        <managed-bean-class>bpelmonitor.ProcessMonitor</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>TopFrame</managed-bean-name>
        <managed-bean-class>bpelmonitor.TopFrame</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>BpelMonitor</managed-bean-name>
        <managed-bean-class>bpelmonitor.BpelMonitor</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Instances</managed-bean-name>
        <managed-bean-class>bpelmonitor.Instances</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/BpelMonitor.jsp</from-view-id>
        <navigation-case>
            <from-outcome>processMonitor</from-outcome>
            <to-view-id>/ProcessMonitor.iface</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ProcessMonitor.jsp</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/BpelMonitor.iface</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>instances</from-outcome>
            <to-view-id>/Instances.iface</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>instances1</from-outcome>
            <to-view-id>/Instances1.iface</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Instances.jsp</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/BpelMonitor.iface</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>processMonitor</from-outcome>
            <to-view-id>/ProcessMonitor.iface</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>Instances1</managed-bean-name>
        <managed-bean-class>bpelmonitor.Instances1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>