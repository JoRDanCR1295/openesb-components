<?xml version="1.0" encoding="UTF-8"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi jbi.xsd">
    <services binding-component="true"
              xmlns:ale="http://www.sun.com/ALE/1.0/ALEService.wsdl">
        <consumes endpoint-name="ALELoggingServicePort" interface-name="ale:ALEServiceLoggingPortType"
        		  service-name="ale:ALELoggingService"/>
        <consumes endpoint-name="ALEAlertServicePort" interface-name="ale:ALEServiceAlertPortType"
        		  service-name="ale:ALEAlertService"/>
        <consumes endpoint-name="ALEErrorServicePort" interface-name="ale:ALEServiceErrorPortType"
        		  service-name="ale:ALEErrorService"/>
    </services>
</jbi>
