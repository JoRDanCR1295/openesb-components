<?xml version="1.0" encoding="UTF-8"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	version="1.0"
	xsi:schemaLocation="http://java.sun.com/xml/ns/jbi jbi.xsd"
	xmlns:ns0="http://www.sun.com/ALE/1.0/ALEService.wsdl">
    <services binding-component="false" >
        <provides endpoint-name="ALELoggingEndpoint" interface-name="ns0:ALEServiceLoggingPortType"
                  service-name="ns0:LoggingService"/>
        <provides endpoint-name="ALEAlertEndpoint" interface-name="ns0:ALEServiceAlertPortType"
                  service-name="ns0:AlertService"/>
        <provides endpoint-name="ALEErrorEndpoint" interface-name="ns0:ALEServiceErrorPortType"
                  service-name="ns0:ErrorService"/>
    </services>
</jbi>
