<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="WebApplication1PU" transaction-type="JTA">
    <jta-data-source>wlm</jta-data-source>
    <class>entity.Assignment</class>
    <class>entity.Instance</class>
    <class>entity.State</class>
    <class>entity.Task</class>
    <class>entity.Timer</class>
    <class>entity.Variable</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
</persistence>
