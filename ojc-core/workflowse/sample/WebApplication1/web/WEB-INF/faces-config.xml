<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>backing</managed-bean-name>
        <managed-bean-class>ColumnsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>panelDividerBean</managed-bean-name>
        <managed-bean-class>PanelDividerBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>taskFilterActionsBean</managed-bean-name>
        <managed-bean-class>selection.TaskFilterActionsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>newTasksBean</managed-bean-name>
        <managed-bean-class>runtime.NewTasksBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>staticTabbedPaneExample</managed-bean-name>
        <managed-bean-class>StaticTabSetBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>borderLayout</managed-bean-name>
        <managed-bean-class>PanelBorderBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>textFields</managed-bean-name>
        <managed-bean-class>TextFieldsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>buttonsAndLinks</managed-bean-name>
        <managed-bean-class>runtime.ButtonsAndLinks</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/wlmHome.jsp</from-view-id>
        <navigation-case>
            <from-outcome>userHome</from-outcome>
            <to-view-id>/userHome.iface</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/userHome.jsp</from-view-id>
        <navigation-case>
            <from-outcome>wlmHome</from-outcome>
            <to-view-id>/wlmHome.iface</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>