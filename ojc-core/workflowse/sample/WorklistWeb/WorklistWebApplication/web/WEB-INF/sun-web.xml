<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 Servlet 2.5//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_2_5-0.dtd">
<sun-web-app error-url="">
  <context-root>/WorklistWebApplication</context-root>
  <security-role-mapping>
    <role-name>administrator</role-name>
    <group-name>administrator</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>CustomerServiceRep</role-name>
    <group-name>CustomerServiceRep</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Supervisor</role-name>
    <group-name>Supervisor</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Manager</role-name>
    <group-name>Manager</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>WLMGroujp</role-name>
    <group-name>WLMGroup</group-name>
  </security-role-mapping>
  <session-config>
    <session-manager persistence-type="memory"/>
  </session-config>
  <class-loader delegate="false"/>
  <jsp-config>
    <property name="classdebuginfo" value="true">
      <description>Enable debug info compilation in the generated servlet class</description>
    </property>
    <property name="mappedfile" value="true">
      <description>Maintain a one-to-one correspondence between static content and the generated servlet class' java code</description>
    </property>
  </jsp-config>
</sun-web-app>
