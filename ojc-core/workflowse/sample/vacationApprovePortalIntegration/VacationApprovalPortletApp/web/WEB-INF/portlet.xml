<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd/etc/opt/SUNWportal/dtd/portlet.xsd" version="1.0">


 <portlet>
        <description>Vocation Approval Portlet</description>
        <portlet-name>VocationApprovalPortlet</portlet-name>
        <display-name>Vocation Approval Portlet</display-name>
        <portlet-class>com.sun.wlm.portal.portlet.VocationApprovalPortlet</portlet-class>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>          
            <portlet-mode>VIEW</portlet-mode>
            <portlet-mode>EDIT</portlet-mode>
            <portlet-mode>HELP</portlet-mode>
        </supports>
        <portlet-info>
            <title>Vocation Approval Portlet</title>
            <short-title>Vocation Approval Portlet</short-title>
        </portlet-info>
 </portlet>

 <portlet>
        <description>Leave Application Portlet</description>
        <portlet-name>LeaveApplicationPortlet</portlet-name>
        <display-name>Leave Application Portlet</display-name>
        <portlet-class>com.sun.wlm.portal.portlet.LeaveApplicationPortlet</portlet-class>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>          
            <portlet-mode>VIEW</portlet-mode>
            <portlet-mode>EDIT</portlet-mode>
            <portlet-mode>HELP</portlet-mode>
        </supports>
        <portlet-info>
            <title>Leave Application Portlet</title>
            <short-title>Leave Application Portlet</short-title>
        </portlet-info>
 </portlet>
</portlet-app>
