<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    
    <managed-bean>  
        <managed-bean-name>TreeBuilderBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.core.TreeBuilderBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean> 

    <managed-bean>  
        <managed-bean-name>PreferencesBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.preferences.PreferencesBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean> 
    
    <managed-bean>  
        <managed-bean-name>SimulatorBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.simulator.SimulatorBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean> 
    
    <managed-bean>  
        <managed-bean-name>ChartStatistics</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.generic.ChartStatistics</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean>    

    <managed-bean>  
        <managed-bean-name>StatisticsBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.generic.StatisticsBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean>    

    
    <managed-bean>  
        <managed-bean-name>ConfigurationBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.generic.ConfigurationBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean>       
    
    <managed-bean>  
        <managed-bean-name>ControlBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.generic.ControlBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean>
    
    <managed-bean>  
        <managed-bean-name>TabsBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.generic.TabsBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean>     
    
     <managed-bean>  
        <managed-bean-name>SVGRendererBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.renderers.svg.SVGRendererBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean>     

    <managed-bean>  
        <managed-bean-name>SAControlBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.renderers.svg.SAControlBean</managed-bean-class>    
            <managed-bean-scope>session</managed-bean-scope>    
    </managed-bean>     

    <managed-bean>  
        <managed-bean-name>ServiceUnitsBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.generic.ServiceUnitsBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean>     

    <managed-bean>  
        <managed-bean-name>PluginsManagerBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.plugins.PluginsManagerBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean>     

    <managed-bean>  
        <managed-bean-name>FileUploaderBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.plugins.FileUploaderBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean>
    
    <managed-bean>
	<managed-bean-name>BannerBean</managed-bean-name>
	<managed-bean-class>com.sun.jbi.cam.manager.framework.core.BannerBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
	<managed-bean-name>CommonTasksBean</managed-bean-name>
	<managed-bean-class>com.sun.jbi.cam.manager.framework.CommonTasksBean</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
    </managed-bean>    
    
    <managed-bean>
        <description></description>
    	<managed-bean-name>JavaHelpBean</managed-bean-name>
    	<managed-bean-class>com.sun.webui.jsf.bean.HelpBackingBean</managed-bean-class>
    	<managed-bean-scope>request</managed-bean-scope>
    </managed-bean>   
    
    <managed-bean>  
        <managed-bean-name>AspectsTabsBean</managed-bean-name>    
            <managed-bean-class>com.sun.jbi.cam.manager.framework.aspects.AspectsTabsBean</managed-bean-class>    
            <managed-bean-scope>application</managed-bean-scope>    
    </managed-bean>     
    
    
    <navigation-rule>
        <from-view-id>/manager/framework/core/pages/tree.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/manager/framework/core/pages/tree.jsp</to-view-id>
        </navigation-case>
    </navigation-rule> 

    <navigation-rule>
        <from-view-id>/manager/framework/generic/control.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/manager/framework/generic/control.jsp</to-view-id>
        </navigation-case>
    </navigation-rule> 

    <navigation-rule>
        <from-view-id>/manager/framework/generic/statistics.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/manager/framework/generic/ChartStatistics.jsp</to-view-id>
        </navigation-case>
    </navigation-rule> 
    
    <navigation-rule>
        <from-view-id>/manager/framework/generic/ChartStatistics.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/manager/framework/generic/statistics.jsp</to-view-id>
        </navigation-case>
    </navigation-rule> 

    <navigation-rule>
        <from-view-id>/manager/framework/core/pages/banner.jsp</from-view-id>
        <navigation-case>
            <from-outcome>redirect-login</from-outcome>
            <to-view-id>/logout.jsp</to-view-id>
        </navigation-case>
    </navigation-rule> 
    
    <navigation-rule>
        <from-view-id>/manager/framework/commonTasks.jsp</from-view-id>
        <navigation-case>
            <from-outcome>overview</from-outcome>
            <to-view-id>/manager/framework/docs/overview.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tests</from-outcome>
            <to-view-id>/manager/framework/simulator/simulator.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deployments</from-outcome>
            <to-view-id>/manager/framework/plugins/tabsFrame.jsp</to-view-id>
        </navigation-case>        
        <navigation-case>
            <from-outcome>configurations</from-outcome>
            <to-view-id>/manager/framework/preferences/preferences.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>aspects</from-outcome>
            <!-- @todo fix this -->
            <!--<to-view-id>/../aspects/aspects/aspects.jsp</to-view-id>-->
            <to-view-id>/manager/framework/aspects/tabsFrame.jsp</to-view-id>
        </navigation-case>
    </navigation-rule> 
  
    <navigation-rule>
        <from-view-id>/manager/framework/renderer/SAControl.jsp</from-view-id>
        <navigation-case>
            <from-outcome>toTextView</from-outcome>
            <to-view-id>/manager/framework/renderer/SATextViewer.jsp</to-view-id>
            <redirect/>
        </navigation-case>
       <navigation-case>
            <from-outcome>toSVGView</from-outcome>
            <to-view-id>/manager/framework/renderer/SASplitViewer.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule> 
    
</faces-config>
