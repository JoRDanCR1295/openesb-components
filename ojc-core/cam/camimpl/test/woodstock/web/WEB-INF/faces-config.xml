<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

    <managed-bean>    
        <description>Table</description>
        <managed-bean-name>TableBean</managed-bean-name>
        <managed-bean-class>woodstock.table.TableBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>     
    </managed-bean> 
    <managed-bean>    
        <description>Table2</description>
        <managed-bean-name>TableBean2</managed-bean-name>
        <managed-bean-class>woodstock.table.TableBean2</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>     
    </managed-bean> 
    
    <managed-bean>    
        <description>Upload</description>
        <managed-bean-name>UploadBean</managed-bean-name>
        <managed-bean-class>woodstock.upload.UploadBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>     
    </managed-bean> 
    
    <managed-bean>    
        <description>Tabs</description>
        <managed-bean-name>TabsBean</managed-bean-name>
        <managed-bean-class>woodstock.tabs.TabsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>     
    </managed-bean>    
    
    <managed-bean>    
        <description>RadioButtons</description>
        <managed-bean-name>RadioButtonsBean</managed-bean-name>
        <managed-bean-class>woodstock.radioButtons.RadioButtonsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>     
    </managed-bean>   
    
</faces-config>
