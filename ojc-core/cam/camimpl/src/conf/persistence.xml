<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="cambpelsePU" transaction-type="JTA">
    <jta-data-source>jdbc/camBpelseDS</jta-data-source>
    <class>com.sun.jbi.cam.plugins.bpelse.datamodel.Engine</class>
    <class>com.sun.jbi.cam.plugins.bpelse.datamodel.Scopesnapshot</class>
    <class>com.sun.jbi.cam.plugins.bpelse.datamodel.State</class>
    <class>com.sun.jbi.cam.plugins.bpelse.datamodel.Variable</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
</persistence>
