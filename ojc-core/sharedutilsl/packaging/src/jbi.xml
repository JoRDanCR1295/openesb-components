<?xml version="1.0"?>
<!-- jbi.xml file for a shared library. -->
<jbi version="1.0"
     xmlns="http://java.sun.com/xml/ns/jbi"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<!-- identification information about this shared library-->
    <shared-library class-loader-delegation="self-first">
        <identification>
            <name>sun-shared-util-library</name>
            <description>A collection of shared utilities for JBI components.</description>
        </identification>
        <shared-library-class-path>
            <!--
            <path-element>wsdlmodel.jar</path-element>
            <path-element>xsdmodel.jar</path-element>
            <path-element>.</path-element>
            <path-element>transform-engine.jar</path-element>
            -->
            <path-element>wsdl4j.jar</path-element>
            <path-element>wsdl4jext.jar</path-element>
            <path-element>crl.jar</path-element>
            <path-element>common-util.jar</path-element>
            <path-element>qos.jar</path-element>
            <path-element>tale.jar</path-element>
            <path-element>component-toolkit.jar</path-element>
            <!--path-element>jsr173_api.jar</path-element>
            <path-element>xbean.jar</path-element>
            <path-element>xbean_xpath.jar</path-element>
            <path-element>xmlpublic.jar</path-element-->
            <path-element>customized-xmlbeans.jar</path-element>
        </shared-library-class-path>
    </shared-library>
</jbi>
