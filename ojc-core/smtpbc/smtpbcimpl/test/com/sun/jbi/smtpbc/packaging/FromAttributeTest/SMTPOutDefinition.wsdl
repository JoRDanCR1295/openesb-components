<definitions targetNamespace="urn:SMTPBCTests/FromAttributeTest/SMTPOutDefinition"
             name="SMTPOutDefinition"
             xmlns:tns="urn:SMTPBCTests/FromAttributeTest/SMTPOutDefinition"
             xmlns:slnk1="http://schemas.xmlsoap.org/ws/2004/03/partner-link/"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:smtp="http://schemas.sun.com/jbi/wsdl-extensions/smtp/"
             xmlns:slnk="http://schemas.xmlsoap.org/ws/2002/07/service-link">
    <plnk:partnerLinkType name="SMTPOutDefinitionSLT"
                          xmlns:plnk="http://schemas.xmlsoap.org/ws/2004/03/partner-link/">
        <plnk:role name="Client"
                   portType="tns:portType_0"></plnk:role>
    </plnk:partnerLinkType>
    <message name="message_0">
        <part name="message"
              type="xsd:string"></part>
        <part name="subject"
              type="xsd:string"></part>
        <part name="from"
              type="xsd:string"></part>
    </message>
    <portType name="portType_0">
        <operation name="operation_0">
            <input name="input"
                   message="tns:message_0"></input>
        </operation>
    </portType>
    <binding name="binding_0"
             type="tns:portType_0">
        <smtp:binding></smtp:binding>
        <operation name="operation_0">
            <smtp:operation></smtp:operation>
            <input name="input">
                <smtp:input message="message"
                            from="from"
                            subject="subject"></smtp:input>
            </input>
        </operation>
    </binding>
    <service name="service_0">
        <port name="port_0"
              binding="tns:binding_0">
            <smtp:address location="mailto:FromAttributeTest@localhost" smtpserver="localhost"></smtp:address>
        </port>
    </service>
</definitions>