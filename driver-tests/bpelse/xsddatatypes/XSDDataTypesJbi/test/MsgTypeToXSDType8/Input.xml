<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:xsd="urn:SampleXSD">
  <soapenv:Body>
    <xsd:person name="John">
      <xsd:home country="US">
        <xsd:name>John</xsd:name>
        <xsd:street>123 Street</xsd:street>
        <xsd:city>LA</xsd:city>
        <xsd:state>CA</xsd:state>
        <xsd:zip>12345</xsd:zip>
      </xsd:home>
      <!--Optional:-->
      <xsd:work country="US">
        <xsd:name>Jane</xsd:name>
        <xsd:street>456 Street</xsd:street>
        <xsd:city>NY</xsd:city>
        <xsd:state>NY</xsd:state>
        <xsd:zip>56789</xsd:zip>
      </xsd:work>
    </xsd:person>
  </soapenv:Body>
</soapenv:Envelope>