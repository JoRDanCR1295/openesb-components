<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns="http://fgov.be/rsvz/definitions"
    targetNamespace="http://fgov.be/rsvz/definitions"
    attributeFormDefault="unqualified" elementFormDefault="qualified">
    <xs:element name="LegalInfoChange">
        <xs:annotation>
            <xs:documentation>Wijziging wettelijke gegevens</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="OldINSS" minOccurs="0" type="INSSType">
                    <xs:annotation>
                        <xs:documentation>Te wijzigen INSZ nummer</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Name" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Nieuwe naam</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Name" type="NameType"/>
                            <xs:element ref="BeginDate"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element ref="Company" minOccurs="0"/>
                <xs:element ref="LinguisticRoleCode" minOccurs="0"/>
                <xs:element ref="OfficialAddress" minOccurs="0"/>
                <xs:element ref="Nationality" minOccurs="0"/>
                <xs:element ref="CivilState" minOccurs="0"/>
                <xs:element ref="Birth" minOccurs="0"/>
                <xs:element ref="Death" minOccurs="0"/>
                <xs:element ref="Gender" minOccurs="0"/>
                <xs:element ref="Profession" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="IncomeCode">
        <xs:annotation>
            <xs:documentation>Code inkomsten</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="4"/>
                <xs:enumeration value="3"/>
                <xs:enumeration value="2"/>
                <xs:enumeration value="1"/>
                <xs:enumeration value="?"/>
                <xs:enumeration value="A"/>
                <xs:enumeration value="N"/>
                <xs:enumeration value="X"/>
                <xs:enumeration value="Y"/>
                <xs:enumeration value="Z"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="LinguisticRoleCodeResidence">
        <xs:annotation>
            <xs:documentation>Taalcode residentie</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:length value="2"/>
                <xs:enumeration value="N0">
                    <xs:annotation>
                        <xs:documentation>Nederlands</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="F0">
                    <xs:annotation>
                        <xs:documentation>Franstalig</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="D0">
                    <xs:annotation>
                        <xs:documentation>Duitstalig</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="N1">
                    <xs:annotation>
                        <xs:documentation>Nederlands met Franse faciliteiten</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="F1">
                    <xs:annotation>
                        <xs:documentation>Frans met Nederlandse faciliteiten</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="F3">
                    <xs:annotation>
                        <xs:documentation>Frans met Duitse faciliteiten</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="F4">
                    <xs:annotation>
                        <xs:documentation>Frans met Nederlands en Duitse faciliteiten</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="B1">
                    <xs:annotation>
                        <xs:documentation>Nederlands en Frans (19
                            Brusselse gemeenten)</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="D2">
                    <xs:annotation>
                        <xs:documentation>Duits met Franse faciliteiten</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="Subjugation">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="SubjugationCode">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:length value="1"/>
                            <xs:enumeration value="0"/>
                            <xs:enumeration value="5"/>
                            <xs:enumeration value="8"/>
                            <xs:enumeration value="9"/>
                            <xs:enumeration value=""/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element ref="BeginDate"/>
                <xs:element name="SubjugationMotivation" minOccurs="0">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="5"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="SignatureDate" type="xs:date">
        <xs:annotation>
            <xs:documentation>Ondertekeningsdatum</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="AllowanceBaseHandicapped">
        <xs:annotation>
            <xs:documentation>Basisbijslag voor gehandicapte kinderen</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="0">
                    <xs:annotation>
                        <xs:documentation>neen of zonder voorwerp</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="1">
                    <xs:annotation>
                        <xs:documentation>gewoon tarief (art. 20, §1,
                            lid 1)</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="2">
                    <xs:annotation>
                        <xs:documentation>verhoogd tarief (art. 20, §1,
                            lid 2 en 3)</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="Acquittal">
        <xs:annotation>
            <xs:documentation>Kwijtschelding</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="SIFCode"/>
                <xs:element ref="RequestDate"/>
                <xs:element name="SummonsDate" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Datum dagvaarding</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="RequestType">
                    <xs:annotation>
                        <xs:documentation>Type aanvraag</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value=""/>
                            <xs:enumeration value="C"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="Quarter" type="QuarterType"/>
                <xs:element name="ContributionType">
                    <xs:annotation>
                        <xs:documentation>Aard bijdrage</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="1">
                                <xs:annotation>
                                    <xs:documentation>voorlopige bijdrage</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="2">
                                <xs:annotation>
                                    <xs:documentation>definitieve bijdrage</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="3">
                                <xs:annotation>
                                    <xs:documentation>regularisatiebijdrage</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="ContributionDemanded" type="xs:decimal">
                    <xs:annotation>
                        <xs:documentation>Bedrag ingevorderde bijdrage</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="ContributionNotPaid" type="xs:decimal">
                    <xs:annotation>
                        <xs:documentation>Bedrag openstaande bijdrage</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="LastPaymentDate" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Datum laatste betaling</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="QuarterlyIncreaseDemanded" type="xs:decimal">
                    <xs:annotation>
                        <xs:documentation>Bedrag ingevorderde
                            verhogingen 3%</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="YearlyIncreaseDemanded" type="xs:decimal">
                    <xs:annotation>
                        <xs:documentation>Bedrag ingevorderde
                            verhogingen 7%</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="IncreaseNotPaid" type="xs:decimal">
                    <xs:annotation>
                        <xs:documentation>Bedrag openstaande verhogingen</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="IncreaseCalculationDate" type="xs:decimal">
                    <xs:annotation>
                        <xs:documentation>Datum berekening verhogingen</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="PostCode">
        <xs:annotation>
            <xs:documentation>Postcode</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="10"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="DocumentType">
        <xs:annotation>
            <xs:documentation>Aard van het document</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:length value="1"/>
                <xs:enumeration value="1">
                    <xs:annotation>
                        <xs:documentation>aangetekende brief</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="2">
                    <xs:annotation>
                        <xs:documentation>verzoekschrift</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="StreetName">
        <xs:annotation>
            <xs:documentation>Straatnaam</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="32"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="BancruptcyInsurancePayment">
        <xs:annotation>
            <xs:documentation>Uitbetaalde premie faillissementsverzekering</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="RequestDate"/>
                <xs:element name="AttributionAmountCode">
                    <xs:annotation>
                        <xs:documentation>Code toekenning bedrag</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:length value="1"/>
                            <xs:enumeration value="0">
                                <xs:annotation>
                                    <xs:documentation>geen uitbetaling toegekend</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="1">
                                <xs:annotation>
                                    <xs:documentation>uitbetaling toegekend</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="2">
                                <xs:annotation>
                                    <xs:documentation>procedure terugvordering</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="3">
                                <xs:annotation>
                                    <xs:documentation>terugvordering</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="4">
                                <xs:annotation>
                                    <xs:documentation>procedure
                                    verzaking aan de terugvordering</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="5">
                                <xs:annotation>
                                    <xs:documentation>verzaking aan de terugvordering</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="6">
                                <xs:annotation>
                                    <xs:documentation>verzaking aan de
                                    terugvordering geweigerd</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="PersonsAtAccountCode" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation>Code personen ten laste</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="Amount"/>
                <xs:element name="MonetaryUnit" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Munteenheid</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="5"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element ref="BeginDate"/>
                <xs:element ref="EndDate"/>
                <xs:element ref="SIFCode"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="CityNisCode">
        <xs:annotation>
            <xs:documentation>NIScode</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="7"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="BancruptcyInsurancePeriod">
        <xs:annotation>
            <xs:documentation>Periode faillissementsverzekering</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="RequestDate"/>
                <xs:element ref="SIFCode"/>
                <xs:element name="DecisionCode">
                    <xs:annotation>
                        <xs:documentation>Beslissingscode</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:length value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element ref="BeginDate"/>
                <xs:element ref="EndDate"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="AffiliateRequest">
        <xs:annotation>
            <xs:documentation>Aanvraag nieuwe aansluiting </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="INSS"/>
                <xs:element ref="LegalInfo"/>
                <xs:element ref="Membership" maxOccurs="unbounded"/>
                <xs:element name="CompanyRepresentative" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Mandataris</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="0">
                                <xs:annotation>
                                    <xs:documentation>vernietigen
                                    bestaande code mandataris</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="1">
                                <xs:annotation>
                                    <xs:documentation>invoeren code mandataris</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element ref="NISSEQualityCode"/>
                <xs:element ref="SignatureDate"/>
                <xs:element ref="SuppressionAssistingSpouse" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="RequestInitiatorID">
        <xs:annotation>
            <xs:documentation>Identificatie van de toepassing van de afzender</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="11"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="Company">
        <xs:annotation>
            <xs:documentation>Onderneming</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="CompanyID"/>
                <xs:element name="CompanyName" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Naam onderneming</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string"/>
                    </xs:simpleType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="Relationship">
        <xs:annotation>
            <xs:documentation>Relatie</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Century" minOccurs="0"/>
                <xs:element ref="INSS"/>
                <xs:element name="QualityRelationCode">
                    <xs:annotation>
                        <xs:documentation>Code hoedanigheid relatie</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="1"/>
                            <xs:enumeration value="2"/>
                            <xs:enumeration value="3"/>
                            <xs:enumeration value="4"/>
                            <xs:enumeration value="5"/>
                            <xs:enumeration value="6"/>
                            <xs:enumeration value="7"/>
                            <xs:enumeration value="8"/>
                            <xs:enumeration value="S"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element ref="BeginDate"/>
                <xs:element ref="EndDate" minOccurs="0"/>
                <xs:element ref="AllowanceBaseHandicapped" minOccurs="0"/>
                <xs:element ref="AbilityDegree" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="NISSEQualityCode">
        <xs:annotation>
            <xs:documentation>Code hoedanigheid</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="1">
                    <xs:annotation>
                        <xs:documentation>zelfstandige</xs:documentation>
                        <xs:documentation>Travailleur indépendant</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="2">
                    <xs:annotation>
                        <xs:documentation>help(st)er</xs:documentation>
                        <xs:documentation>Aidant ou aidante</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="3">
                    <xs:annotation>
                        <xs:documentation>art. 12</xs:documentation>
                        <xs:documentation>Application art. 12</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="4">
                    <xs:annotation>
                        <xs:documentation>meewerkende echtgeno(o)t(e)</xs:documentation>
                        <xs:documentation>Conjoint aidant</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="Century">
        <xs:annotation>
            <xs:documentation>Eeuw (variante rreeuw)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:length value="2"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="SDIAttestationNbr">
        <xs:annotation>
            <xs:documentation>Nummer ZIV bon of attest</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:maxLength value="15"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="ContributionOrder">
        <xs:annotation>
            <xs:documentation>Bijdragebon</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Sector" maxOccurs="2">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:length value="3"/>
                            <xs:enumeration value="012">
                                <xs:annotation>
                                    <xs:documentation>RSZ</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                            <xs:enumeration value="013">
                                <xs:annotation>
                                    <xs:documentation>RSZ-PPO</xs:documentation>
                                </xs:annotation>
                            </xs:enumeration>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="Activity" minOccurs="0" maxOccurs="4">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="WorkingDays" type="xs:decimal"/>
                            <xs:element ref="Quarter"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element ref="X001" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="CancelAffiliationRequest">
        <xs:annotation>
            <xs:documentation>Annulatie aanvraag aansluiting</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="AffiliationNbr"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="BancruptcyInsuranceRequest">
        <xs:annotation>
            <xs:documentation>Aanvraag faillissementsverzekering</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="SIFCode"/>
                <xs:element ref="BeginDate"/>
                <xs:element ref="DocumentType"/>
                <xs:element name="ActionCode">
                    <xs:annotation>
                        <xs:documentation>Code invoeren/vernietigen </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:length value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element ref="RequestDate"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="JuridicalFoundation">
        <xs:annotation>
            <xs:documentation>Rechtsgrond bij KB</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="JuridicalFoundationCode">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:length value="1"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="JuridicalFoundationDesc" minOccurs="0" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="LinguisticRoleCode">
        <xs:annotation>
            <xs:documentation>Taalcode</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:length value="1"/>
                <xs:enumeration value="0">
                    <xs:annotation>
                        <xs:documentation>onbekend</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="1">
                    <xs:annotation>
                        <xs:documentation>franstalig</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="2">
                    <xs:annotation>
                        <xs:documentation>nederlandstalig</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
                <xs:enumeration value="3">
                    <xs:annotation>
                        <xs:documentation>duitstalig</xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="EndDate" type="xs:date">
        <xs:annotation>
            <xs:documentation>Einddatum</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="SuppressionAssistingSpouse">
        <xs:annotation>
            <xs:documentation>Schrapping meewerkende echtgenoot</xs:documentation>
            <xs:documentation>Annulation affiliation en qualité de
                conjoint aidant</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="LastAffiliationDay" minOccurs="0" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Laatste dag van de
