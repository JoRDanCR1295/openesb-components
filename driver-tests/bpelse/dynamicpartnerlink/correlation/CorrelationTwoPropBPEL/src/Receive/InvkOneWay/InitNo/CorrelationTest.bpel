<?xml version="1.0" encoding="UTF-8"?>
<process
    name="CorrelationTest"
    targetNamespace="http://enterprise.netbeans.org/bpel/Receive_InvkOneWay_InitNo/src/CorrelationTest"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:bpws="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:wsdlNS="http://enterprise.netbeans.org/bpel/Receive_InvkOneWay_InitNo/src/CorrelationTest" xmlns:ns1="http://j2ee.netbeans.org/wsdl/Receive_InvkOneWay_InitNo/src/CorrelationTest" xmlns:ns0="http://xml.netbeans.org/schema/Receive_InvkOneWay_InitNo/src/Message">
    <bpws:import namespace="http://j2ee.netbeans.org/wsdl/Receive_InvkOneWay_InitNo/src/CorrelationTest" location="CorrelationTest.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <bpws:partnerLinks>
        <bpws:partnerLink name="PartnerLinkB" partnerLinkType="ns1:SubBPPartner" partnerRole="SubBPPortTypeRole"/>
        <bpws:partnerLink name="PartnerLinkA" partnerLinkType="ns1:CorrelationTestPartner" myRole="CorrelationTestPortTypeRole"/>
    </bpws:partnerLinks>
    <bpws:variables>
        <bpws:variable name="CorrelationReturnOperationIn" messageType="ns1:CorrMessage"/>
        <bpws:variable name="InitiateProcessOperationIn" messageType="ns1:Message"/>
    </bpws:variables>
    <bpws:correlationSets>
        <bpws:correlationSet name="CorrelationSet" properties="ns1:CorrelationId1 ns1:CorrelationId2"/>
    </bpws:correlationSets>
    <sequence>
        <bpws:receive name="Receive1" createInstance="yes" partnerLink="PartnerLinkA" operation="InitiateProcessOperation" portType="ns1:CorrelationTestPortType" variable="InitiateProcessOperationIn"/>
        <bpws:invoke name="Invoke1" partnerLink="PartnerLinkB" operation="CorrelationTestOperation" portType="ns1:SubBPPortType" inputVariable="InitiateProcessOperationIn">
            <bpws:correlations>
                <bpws:correlation set="CorrelationSet" initiate="yes"/>
            </bpws:correlations>
        </bpws:invoke>
        <bpws:receive name="Receive2" partnerLink="PartnerLinkA" operation="CorrelationReturnOperation" portType="ns1:CorrelationTestPortType" createInstance="no" variable="CorrelationReturnOperationIn">
            <bpws:correlations>
                <bpws:correlation set="CorrelationSet" initiate="no"/>
            </bpws:correlations>
        </bpws:receive>
        <bpws:assign name="Assign1">
            <bpws:copy>
                <bpws:from>$CorrelationReturnOperationIn.Data/ns0:Content</bpws:from>
                <bpws:to>$InitiateProcessOperationIn.Data/ns0:Content</bpws:to>
            </bpws:copy>
        </bpws:assign>
        <bpws:reply name="Reply1" partnerLink="PartnerLinkA" operation="InitiateProcessOperation" portType="ns1:CorrelationTestPortType" variable="InitiateProcessOperationIn"/>
    </sequence>
</process>
