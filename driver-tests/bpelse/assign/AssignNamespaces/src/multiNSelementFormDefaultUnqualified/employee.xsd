<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://xml.netbeans.org/schema/multiNSelementFormDefaultUnqualified/employee"
            xmlns:tns="http://xml.netbeans.org/schema/multiNSelementFormDefaultUnqualified/employee"
            xmlns:ns0="http://xml.netbeans.org/schema/multiNSelementFormDefaultUnqualified/person">
    <xsd:import schemaLocation="person.xsd" namespace="http://xml.netbeans.org/schema/multiNSelementFormDefaultUnqualified/person"/>
    <xsd:element name="Employee">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="ns0:person"/>
                <xsd:element name="experience" type="xsd:int"></xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
