<?xml version="1.0" encoding="UTF-8"?>
<process
    name="main"
    targetNamespace="http://enterprise.netbeans.org/bpel/requestlifespan/onevent/main"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling"
    xmlns:tns="http://enterprise.netbeans.org/bpel/requestlifespan/onevent/main" xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor">
   <import namespace="http://j2ee.netbeans.org/wsdl/requestlifespan/onevent/wsdl" location="wsdl.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
   <partnerLinks>
      <partnerLink name="PartnerLink2" xmlns:tns="http://j2ee.netbeans.org/wsdl/requestlifespan/onevent/wsdl" partnerLinkType="tns:subOne" partnerRole="subPortTypeRole"/>
      <partnerLink name="PartnerLink1" xmlns:tns="http://j2ee.netbeans.org/wsdl/requestlifespan/onevent/wsdl" partnerLinkType="tns:main" myRole="mainPortTypeRole"/>
   </partnerLinks>
   <variables>
      <variable name="MainOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/requestlifespan/onevent/wsdl" messageType="tns:operationResponse"/>
      <variable name="MainOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/requestlifespan/onevent/wsdl" messageType="tns:operationRequest"/>
   </variables>
   <sequence>
      <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="mainOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/requestlifespan/onevent/wsdl" portType="tns:mainPortType" variable="MainOperationIn"/>
      <scope name="Scope1" xmlns:tns="http://j2ee.netbeans.org/wsdl/requestlifespan/onevent/wsdl">
         <faultHandlers>
               <catch faultName="sxeh:systemFault" faultVariable="faultVar" faultMessageType="sxeh:faultMessage">
                     <assign name="Assign1">
                        <copy>
                           <!--from>concat('Fault as expected.', $faultVar.fault)</from-->
                           <from variable="faultVar" part="fault"/>
                           <to variable="MainOperationOut" part="output"/>
                        </copy>
                     </assign>
                  </catch>
            </faultHandlers>
            <invoke name="Invoke1" partnerLink="PartnerLink2" operation="subTwoOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/requestlifespan/onevent/wsdl" portType="tns:subPortType" inputVariable="MainOperationIn"/>
      </scope>
      <reply name="Reply1" partnerLink="PartnerLink1" operation="mainOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/requestlifespan/onevent/wsdl" portType="tns:mainPortType" variable="MainOperationOut"/>
   </sequence>
</process>
