#! /bin/sh

echo .
echo STARTING WSTESTCLIENT

if [ "$1" ]

then
  
	THREAD=$1
else

	THREAD=1
fi

if [ "$2" ]

then

	COUNT=$2
else

	COUNT=1
fi

if [ "$3" ]

then

	VERIFY=$3
else

	VERIFY="true"
fi

$JAVA_HOME/bin/java -cp bin:$APPSERVER_HOME/lib/appserv-ws.jar:$APPSERVER_HOME/lib/javaee.jar test.Test http://localhost:18181/purchaseOrderCoordinatorService/purchaseOrderCoordinatorPort $THREAD $COUNT $VERIFY
