<?xml version="1.0" encoding="UTF-8"?>


<wsdl:definitions name="FlowBasic"
             targetNamespace="http://www.mycomp.org/DefaultServiceName"
             xmlns:tns="http://www.mycomp.org/DefaultServiceName"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"                          
             xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
             xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype" xmlns:ns="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:ns0="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:ns1="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:ns2="http://schemas.xmlsoap.org/wsdl/soap/">
    <wsdl:message name="message1">
        <wsdl:part name="branch1" type="xsd:string"/>
        <wsdl:part name="branch2" type="xsd:string"/>
    </wsdl:message>
    <wsdl:portType name="FlowBasicPortType">
        <wsdl:operation name="FlowBasicOperation">
            <wsdl:input name="input1" message="tns:message1"/>
            <wsdl:output name="output1" message="tns:message1"/>
        </wsdl:operation>
    </wsdl:portType>

        <binding name="FlowBasicSoapBinding" type="tns:FlowBasicPortType">
            <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
                <operation name="FlowBasicOperation">
                        <soap:operation style="document"/>
                        <input name="input1">
                     <soap:body/>
                </input><output name="output1">
    <soap:body/>
</output>
                </operation>
        </binding>

   
         <wsdl:service name="FlowBasicService">
		<wsdl:port name="FlowBasicPortName" binding="tns:FlowBasicSoapBinding">                    
                     <soap:address location="http://localhost:${HttpDefaultPort}/FlowBasic"/>
		</wsdl:port>
        </wsdl:service>

	<plnk:partnerLinkType name="FlowBasicPartnerLinkType">
		<plnk:role name="FlowBasicProvider" portType="tns:FlowBasicPortType"/>
	</plnk:partnerLinkType>

</wsdl:definitions>


 

 

 

 

 
