	<xs:complexType name="TradeInFinancing">
		<xs:annotation>
			<xs:appinfo>Derived from oa:FinancingBase</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FinanceType" type="FinanceType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Type of financing Examples may be F- Finance, L - Lease, C-Cash, 1-Retail, 2-Lease, 3-Balloon, etc..</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentAmount" type="PaymentAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Monthly Payment Amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BalanceAmount" type="BalanceAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Remaining balance on Current Vehicle</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinalAmount" type="FinalAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Last month payment amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResidualAmount" type="ResidualAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Remaining residual on current vehicle</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Term" type="Term" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Number of months of contract term</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaturityDate" type="MaturityDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Month and year of contract maturity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinanceCompanyName" type="FinanceCompanyName" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Name of Finance Company</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WholesaleValueAmount" type="WholesaleValueAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Actual wholesale value of the vehicle.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NetTradeAllowanceAmount" type="NetTradeAllowanceAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The gross trade-in amount minus the payoff amount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrossTradeIn" type="GrossTradeIn" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The gross trade-in amount minus the payoff.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PriorCreditOrLeaseOwedOnTrade" type="PriorCreditOrLeaseOwedOnTrade" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The balance owed on lease that is being terminated. This balance is amount being financed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TradeInSalesTaxCredit" type="TradeInSalesTaxCredit" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Rebate on sales tax previously paid on the vehicle.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LeaseMaturityDate" type="LeaseMaturityDate" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The date that the lease matures.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UpfrontNetTradePayoffAmount" type="UpfrontNetTradePayoffAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The balance owed on lease that is being terminated. This balance is amount being paid upfront. This is the amount paid upfront to cover negative equity on a trade-in.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<!-- Rev 1.1 Removed TradeInPayoff MV032603 -->
	</xs:complexType>
