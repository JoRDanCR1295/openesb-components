		<xs:annotation>
			<xs:documentation source="http://www.starstandards.org">Free form text related to option</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Note"/>
	</xs:simpleType>
	<xs:simpleType name="DealerInstallationInd">
		<xs:annotation>
			<xs:documentation source="http://www.starstandards.org">Indicates whether or not the dealership installed the part</xs:documentation>
