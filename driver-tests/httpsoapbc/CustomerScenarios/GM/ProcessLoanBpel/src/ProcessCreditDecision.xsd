	<xs:complexType name="FinancialPosition">
		<xs:sequence>
			<xs:element name="CurrentAssetsAmount" type="CurrentAssetsAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Assets that a company has at its disposal that can be easily converted into cash within one operating cycle.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IntangibleAssetsAmount" type="IntangibleAssetsAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">An asset that is not visible in the traditional balance sheet but still adds value to the company.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FixedAssetsAmount" type="FixedAssetsAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Fixed Assets of organization are those assets of a permanent nature required for the normal conduct of a business, and which will not normally be converted into cash during the ensuring fiscal period. For example, furniture, fixtures, land, and buildings are all fixed assets. However, accounts receivable and inventory are not.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalAssetsAmount" type="TotalAssetsAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The total of all assets; both current and fixed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrentLiabilitiesAmount" type="CurrentLiabilitiesAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The liabilities to be paid within one year of the balance sheet date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LongTermDirectLiabilitiesAmt" type="LongTermDirectLiabilitiesAmt" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Loans, debt or obligations with a maturity of longer than one year.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalLiabilitiesAmount" type="TotalLiabilitiesAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The sum of all liability items.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CapitalAmount" type="CapitalAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The money and other property of a corporation or other enterprise used in transacting the business.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EarnedSurplusAmount" type="EarnedSurplusAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Prior years earnings not paid out as dividends, i.e., Retained Earnings.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AnnualSalesAmount" type="AnnualSalesAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Annual Sales of an organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrossProfitAmount" type="GrossProfitAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The net sales minus cost of sales.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NetProfitAmount" type="NetProfitAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The company's total earnings reflecting revenues adjusted for costs of doing business, depreciation, interest, taxes and other expenses.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WorkingCapitalAmount" type="WorkingCapitalAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The difference between current assets and current liabilities. Measures the margin of protection for current creditors.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalNetWorthAmount" type="TotalNetWorthAmount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The total assets minus the total liabilities.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebtNetWorthRatio" type="DebtNetWorthRatio" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Ratio of Debt to Net Worth of organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrentAssetsLiabilitiesRatio" type="CurrentAssetsLiabilitiesRatio" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Ratio of Current Assets to Liabilities of organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
