	<xs:simpleType name="ApplicationStatus">
		<xs:annotation>
			<xs:documentation source="http://www.starstandards.org">Status of credit application - P = Pending, A = Approved, C = Approved with Conditions, D = Denied, B = Booked, N = New, M = Modified, R = Preliminary, I = Withdrawn, L = Canceled, O = Condition, E = Error, RO = Reopened</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"P" = Pending</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"A" = Approved</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"C" = Approved with Conditions</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"D" = Denied</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="B">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"B" = Booked</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"N" = New</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="M">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"M" = Modified</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"R" = Preliminary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="I">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"I" = Withdrawn</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="L">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"L" = Canceled</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="O">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"O" = Condition</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"E" = Error</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RO">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"RO" = Reopened</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OT">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">"OT" = Other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
		<!-- Rev 1.1 Added "E" = Error MV03/11/03 -->
		<!-- Rev 2.0 Added "RO" = Reopened MV06/25/03 -->
