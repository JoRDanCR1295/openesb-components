	</xs:simpleType>
	<xs:simpleType name="UOM">
		<xs:annotation>
			<xs:documentation source="http://www.starstandards.org">Units of Measure - ea=Each; bx=Box; case=Case; ctn=Carton; gal=Gallon; qt=Quart; pt=Pint; ft=Feet; yd=Yard; in=Inch; L=Liter; m=Meter; cm=Centimeter; kg=Kilograms; g=grams; other=Other</xs:documentation>
