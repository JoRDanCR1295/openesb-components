					<xs:documentation source="http://www.starstandard.org">kilometers</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mi">
				<xs:annotation>
					<xs:documentation source="http://www.starstandard.org">miles</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="hp">
				<xs:annotation>
					<xs:documentation source="http://www.starstandard.org">horsepower</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="kw">
				<xs:annotation>
					<xs:documentation source="http://www.starstandard.org">kilowatt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
