			<xs:extension base="HeaderBase">
				<xs:sequence>
					<xs:element name="DocumentId" type="DocumentId">
						<xs:annotation>
							<xs:documentation source="http://www.starstandards.org">The primary DocumentId of the document for the given content.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicationStatus" type="ApplicationStatus">
