	<xs:complexType name="CreditDetailBase">
		<xs:annotation>
			<xs:documentation source="http://www.starstandards.org">.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VehiclesFound" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">The vehicle that the Applicant is interested in purchasing</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="CreditVehicle">
							<xs:sequence>
								<xs:element name="Vehicle" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="VIN" type="VIN">
												<xs:annotation>
													<xs:documentation source="http://www.starstandards.org">Federally defined 17 position vehicle identification number</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ModelYear" type="ModelYear" minOccurs="0">
												<xs:annotation>
													<xs:documentation source="http://www.starstandards.org">Vehicle designated model year</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="Make" type="Make" minOccurs="0">
												<xs:annotation>
													<xs:documentation source="http://www.starstandards.org">Vehicle make code - Usually available in the VIN number (use NCIC code).</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="Model" type="Model" minOccurs="0">
												<xs:annotation>
													<xs:documentation source="http://www.starstandards.org">Manufacturer-assigned model code of vehicle - Usually available in the VIN number (use NCIC code)</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ExteriorColor" type="ExteriorColor" minOccurs="0" maxOccurs="unbounded">
												<xs:annotation>
													<xs:documentation source="http://www.starstandards.org">Vehicle Exterior Color(s)</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="InteriorColor" type="InteriorColor" minOccurs="0" maxOccurs="unbounded">
												<xs:annotation>
													<xs:documentation source="http://www.starstandards.org">Vehicle Interior Color(s)</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="SourceDealer">
												<xs:complexType>
													<xs:complexContent>
														<xs:extension base="DealerParty"/>
													</xs:complexContent>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="IndividualApplicant" type="IndividualApplicantPartyExtended" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Business party who is applying for vehicle financing</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ApplicationFinancing">
		<xs:sequence>
			<xs:element name="FinanceType" type="FinanceType">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Type of financing Examples may be F- Finance, L - Lease, C-Cash, 1-Retail, 2-Lease, 3-Balloon, etc..</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentAmount" type="PaymentAmount">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Monthly Payment Amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Term" type="Term">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Number of months of contract term</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinanceCompanyName" type="FinanceCompanyName" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="http://www.starstandards.org">Name of Finance Company</xs:documentation>
