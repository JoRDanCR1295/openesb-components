			<xs:extension base="Amount"/>
		</xs:simpleContent>
		<!-- Rev 1.1 Added MV03/11/03 -->
	</xs:complexType>
	<xs:complexType name="TotalAssetsAmount">
		<xs:annotation>
			<xs:documentation source="http://www.starstandards.org">The total of all assets; both current and fixed.</xs:documentation>
