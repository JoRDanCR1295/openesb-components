<?xml version="1.0" encoding="UTF-8"?>
<process 
    name="siebelAcctUpdateBpel" 
    targetNamespace="http://enterprise.netbeans.org/bpel/siebelAcctUpdateBpel"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:bpws="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:wsdlNS="http://enterprise.netbeans.org/bpel/siebelAcctUpdateBpel" xmlns:ns1="http://siebel.com/asi/" xmlns:ns2="http://stc.com/echo" xmlns:ns0="http://www.siebel.com/xml/AccountInterface">
    <import namespace="http://siebel.com/asi/" location="SiebelAccount.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://stc.com/echo" location="echo.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="echoPartnerLink" partnerLinkType="ns2:SLT" myRole="server"/>
        <partnerLink name="siebelPartnerLink" partnerLinkType="ns1:SiebelAcccountpartnerlinktyperole1" partnerRole="SiebelAccountpartnerlink"/>
    </partnerLinks>
    <variables>
        <variable name="EchoOut1" messageType="ns2:echo"/>
        <variable name="SiebelAccountUpdateOut1" messageType="ns1:Siebel_spcAccount_SiebelAccountUpdate_Output"/>
        <variable name="SiebelAccountUpdateIn1" messageType="ns1:Siebel_spcAccount_SiebelAccountUpdate_Input"/>
        <variable name="EchoIn1" messageType="ns2:echo"/>
    </variables>
    <sequence>
        <receive name="EchoReceive" createInstance="yes" partnerLink="echoPartnerLink" operation="echo" portType="ns2:echo" variable="EchoIn1"/>
        <assign name="Assign1">
            <copy>
                <from>'1-2YL0'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:AccountId</to>
            </copy>
            <copy>
                <from>'Active'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:AccountStatus</to>
            </copy>
            <copy>
                <from>'N'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:CompetitorFlag</to>
            </copy>
            <copy>
                <from>'USD'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:CurrencyCode</to>
            </copy>
            <copy>
                <from>'6264716000'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:MainPhoneNumber</to>
            </copy>
            <copy>
                <from>'AccDec1'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:Name</to>
            </copy>
            <copy>
                <from>'USD'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:POAutoApprovalCurrencyCode</to>
            </copy>
            <copy>
                <from>'N'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:PartnerFlag</to>
            </copy>
            <copy>
                <from>'Default Organization'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:PrimaryOrganization</to>
            </copy>
            <copy>
                <from>'N'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:SkipCreditCheck</to>
            </copy>
            <copy>
                <from>' IsPrimaryMVG="Y"'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:ListOfRelatedSalesRep/ns0:RelatedSalesRep[1]</to>
            </copy>
            <copy>
                <from>'Siebel Administrator'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:ListOfRelatedSalesRep/ns0:RelatedSalesRep[1]/ns0:Position</to>
            </copy>
            <copy>
                <from>'0-5220'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:ListOfRelatedSalesRep/ns0:RelatedSalesRep[1]/ns0:PositionId</to>
            </copy>
            <copy>
                <from>'SADMIN'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:ListOfRelatedSalesRep/ns0:RelatedSalesRep[1]/ns0:Login</to>
            </copy>
            <copy>
                <from>'Siebel Administration'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:ListOfRelatedSalesRep/ns0:RelatedSalesRep[1]/ns0:Division</to>
            </copy>
            <copy>
                <from>' IsPrimaryMVG="Y"'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:ListOfRelatedOrganization/ns0:RelatedOrganization[1]</to>
            </copy>
            <copy>
                <from>'Default Organization'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:ListOfRelatedOrganization/ns0:RelatedOrganization[1]/ns0:Organization</to>
            </copy>
            <copy>
                <from>'0-R9NH'</from>
                <to>$SiebelAccountUpdateIn1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:ListOfRelatedOrganization/ns0:RelatedOrganization[1]/ns0:OrganizationId</to>
            </copy>
        </assign>
        <invoke name="InvokeSiebelUpdateAcct" partnerLink="siebelPartnerLink" operation="SiebelAccountUpdate" portType="ns1:Default" inputVariable="SiebelAccountUpdateIn1" outputVariable="SiebelAccountUpdateOut1"/>
        <assign name="Assign2">
            <copy>
                <from>$SiebelAccountUpdateOut1.SiebelMessage/ns0:ListOfAccountInterface/ns0:Account[1]/ns0:AccountId</from>
                <to>$EchoOut1.part/ns2:value</to>
            </copy>
        </assign>
        <reply name="echoReply" partnerLink="echoPartnerLink" operation="echo" portType="ns2:echo" variable="EchoOut1"/>
   </sequence>
   
</process>
