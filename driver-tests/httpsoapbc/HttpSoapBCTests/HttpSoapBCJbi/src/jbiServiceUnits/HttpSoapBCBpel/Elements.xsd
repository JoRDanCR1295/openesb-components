	<xs:complexType name="CareerComponentType">
		<xs:annotation>
			<xs:documentation xml:lang="nl">Datatype voor loopbaanperiode</xs:documentation>
			<xs:documentation xml:lang="fr">Data type Période de carrière </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element ref="BeginDate"/>
				<xs:element name="OldBeginDate" type="xs:date">
					<xs:annotation>
						<xs:documentation xml:lang="nl">Begindatum te verwijderen periode</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element ref="ActivityCode"/>
			<xs:element ref="ActivityCodeDesc" minOccurs="0"/>
			<xs:element ref="ContributionCode"/>
			<xs:element ref="ContributionCodeDesc" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DisablementType">
		<xs:sequence>
			<xs:element ref="INSS"/>
			<xs:element ref="DisablementBeginDate" minOccurs="0"/>
			<xs:element ref="DisablementEndDate" minOccurs="0"/>
			<xs:element ref="ComplementaryData"/>
			<xs:element ref="Allowance"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NameType">
		<xs:annotation>
			<xs:documentation xml:lang="nl">Datatype voor naam</xs:documentation>
			<xs:documentation xml:lang="fr">Datatype Nom</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LastName">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Familienaam</xs:documentation>
					<xs:documentation xml:lang="fr">Nom de famille</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="48"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FirstName" minOccurs="0" maxOccurs="2">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Eerste en tweede voornaam</xs:documentation>
					<xs:documentation xml:lang="fr">Premier et second prénoms</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="24"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FirstNameIndicator" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Indicatie of het om twee verschillende voornamen gaat, of om één opgespliste voornaam.</xs:documentation>
					<xs:documentation xml:lang="fr">Indication s'il s'agit de 2 prénoms différents ou d'un prénom composé</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="0">
							<xs:annotation>
								<xs:documentation xml:lang="fr">il n'y a qu'un seul prénom ou il en a deux dont un est incomplet</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation xml:lang="fr">deux prénoms complets</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Title" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Aanspreektitel</xs:documentation>
					<xs:documentation xml:lang="fr">Titre</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PeriodType">
		<xs:annotation>
			<xs:documentation xml:lang="nl">Datatype voor een periode</xs:documentation>
			<xs:documentation xml:lang="fr">Datatype Période</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="BeginDate"/>
			<xs:element ref="EndDate" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ResidenceType">
		<xs:annotation>
			<xs:documentation xml:lang="nl">Datatype voor een verblijfplaats</xs:documentation>
			<xs:documentation xml:lang="fr">Data type Lieu de résidence</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="CityNisCode"/>
			<xs:element ref="CityName" maxOccurs="2"/>
			<xs:element ref="LinguisticRoleCodeResidence" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AbilityDegree">
		<xs:annotation>
			<xs:documentation xml:lang="nl">Graad van zelfredzaamheid </xs:documentation>
			<xs:documentation xml:lang="fr">Degré d'autonomie</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="0">
					<xs:annotation>
						<xs:documentation xml:lang="nl">geen vaststelling of zonder voorwerp</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="1">
					<xs:annotation>
						<xs:documentation xml:lang="nl">zelfredzaamheid 0 tot en met 3 punten</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="2">
					<xs:annotation>
						<xs:documentation xml:lang="nl">zelfredzaamheid 4 tot en met 6 punten</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="3">
					<xs:annotation>
						<xs:documentation xml:lang="nl">zelfredzaamheid 7 tot en met 9 punten</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Acquittal">
		<xs:annotation>
			<xs:documentation xml:lang="nl">Kwijtschelding</xs:documentation>
			<xs:documentation xml:lang="fr">Renonciation</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SIFCode"/>
				<xs:element ref="RequestDate"/>
				<xs:element name="SummonsDate" type="xs:date" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="nl">Datum dagvaarding</xs:documentation>
						<xs:documentation xml:lang="fr">Date de la citation</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RequestType">
					<xs:annotation>
						<xs:documentation xml:lang="nl">Type aanvraag</xs:documentation>
						<xs:documentation>Type de demande</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="N">
								<xs:annotation>
									<xs:documentation xml:lang="nl">normale aanvraag</xs:documentation>
									<xs:documentation xml:lang="fr">demande normale</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="C">
								<xs:annotation>
									<xs:documentation xml:lang="nl">collectieve schuldenregeling</xs:documentation>
									<xs:documentation xml:lang="fr">règlement collectif de dettes</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="F">
								<xs:annotation>
									<xs:documentation xml:lang="nl">verschoonbaar faillissement</xs:documentation>
									<xs:documentation xml:lang="fr">faillite excusable</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Quarter" type="QuarterType">
					<xs:annotation>
						<xs:documentation xml:lang="nl">Kwartaal</xs:documentation>
						<xs:documentation xml:lang="fr">Trimestre</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ContributionType">
					<xs:annotation>
						<xs:documentation xml:lang="nl">Aard bijdrage</xs:documentation>
						<xs:documentation xml:lang="fr">Type de cotisation</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="1">
								<xs:annotation>
									<xs:documentation xml:lang="nl">voorlopige bijdrage</xs:documentation>
									<xs:documentation xml:lang="fr">cotisation provisoire</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="2">
								<xs:annotation>
									<xs:documentation xml:lang="nl">definitieve bijdrage</xs:documentation>
									<xs:documentation xml:lang="fr">cotisation définitive</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="3">
								<xs:annotation>
									<xs:documentation xml:lang="nl">regularisatiebijdrage</xs:documentation>
									<xs:documentation xml:lang="fr">cotisation de régularisation</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ContributionDemanded" type="xs:decimal">
					<xs:annotation>
