<xs:schema xmlns="http://www.rsvz-inasti.fgov.be/schemas/B2B/Exchanges" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.rsvz-inasti.fgov.be/schemas/B2B/Exchanges" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="BirthDateType">
		<xs:annotation>
			<xs:documentation xml:lang="nl">Datatype voor geboortedatum</xs:documentation>
			<xs:documentation xml:lang="fr">Format date de naissance</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[1-2][0-9]{3}\-[0-1][0-9]\-[0-3][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MonthType">
		<xs:annotation>
			<xs:documentation xml:lang="nl">Datatype voor maand</xs:documentation>
			<xs:documentation xml:lang="fr">Format mois</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[1-9][0-9][0-9][0-9][0-1][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QuarterType">
		<xs:annotation>
			<xs:documentation xml:lang="nl">Datatype voor kwartaal</xs:documentation>
			<xs:documentation xml:lang="fr">Format trimestre CCYYT</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="5"/>
			<xs:pattern value="[1-9][0-9][0-9][0-9][1-4]">
				<xs:annotation>
					<xs:documentation>Valid from 20021 to 20294</xs:documentation>
				</xs:annotation>
			</xs:pattern>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SDICodeType">
		<xs:annotation>
			<xs:documentation xml:lang="nl">Datatype voor ZIV-code</xs:documentation>
			<xs:documentation xml:lang="fr">Type de donnée pour code AMI</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="00">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Niet onderworpen</xs:documentation>
					<xs:documentation xml:lang="fr">Non assujetti</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Zelfstandige in hoofdberoep voor de pensioenleeftijd</xs:documentation>
					<xs:documentation xml:lang="fr">Indépendant à titre principal avant l'âge de la retraite</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Zelfstandige in bijberoep met bijdrage lager dan de minimumbijdrage hoofdberoep</xs:documentation>
					<xs:documentation xml:lang="fr">Indépendant à titre complémentaire avec cotisation inférieure à la cotisation minimum activité principale</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="03">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Zelfstandige in bijberoep met bijdrage hoger of gelijk aan de minimumbijdrage hoofdberoep</xs:documentation>
					<xs:documentation xml:lang="fr">Indépendant à titre complémentaire avec cotisation supérieure ou égale à la cotisation minimum activité principale</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="04">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Zelfstandige die gepensioneerd is of de pensioenleeftijd bereikt heeft, met bijdrageplicht</xs:documentation>
					<xs:documentation xml:lang="fr">Indépendant pensionné ou ayant atteint l'âge de la retraite, redevable d'une cotisation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="05">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Zelfstandige in voortgezette verzekering voor pensioen en ZIV en niet gewezen kolonist</xs:documentation>
					<xs:documentation xml:lang="fr">Indépendant en assurance continuée pour pension et AMI, non ancien colon</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="06">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Zelfstandige in voortgezette verzekering voor pensioen en ZIV en gewezen koloniaal</xs:documentation>
					<xs:documentation xml:lang="fr">Indépendant en assurance continuée pour pension et AMI, ancien colon</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="07">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Onderworpene aan het sociaal statuut met toepassing van art37/40</xs:documentation>
					<xs:documentation xml:lang="fr">Assujetti au statut social avec application de l'article 37/40</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="08">
				<xs:annotation>
					<xs:documentation xml:lang="nl">K.B. van 11.10.1985 wanneer de bijdrage opeisbaar maar niet geïnd is</xs:documentation>
					<xs:documentation xml:lang="fr">A.R. du 11.10.1985 quand la cotisation est exigible mais non encaissée</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="09">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Aanvraag voor gelijkstelling of voortgezette verzekering voor pensioen</xs:documentation>
					<xs:documentation xml:lang="fr">Demande d'assimilation ou assurance continuée pour pension</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="10">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Actieve zelfstandige die gepensioneerd is of zelfstandige actief na de pensioenleeftijd, zonder bijdrageplicht</xs:documentation>
					<xs:documentation xml:lang="fr">Indépendant pensionné actif ou pensionné actif après l'âge de la retraite, non redevable d'une cotisation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="11">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Zelfstandige in bijberoep, zonder bijdrageplicht</xs:documentation>
					<xs:documentation xml:lang="fr">Indépendant à titre complémentaire, non redevable d'une cotisation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="12">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Onderworpene aan het sociaal statuut met toepassing van art 37, zonder bijdrageplicht</xs:documentation>
					<xs:documentation xml:lang="fr">Assujetti au statut social avec application de l'article 37/40, non redevable d'une cotisation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="13">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Zelfstandige met vrijstelling van bijdrage in toepassing van art 22</xs:documentation>
					<xs:documentation xml:lang="fr">Indépendant ayant une dispense de cotisation en application de l'article 22</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="14">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Meewerkende echtgenoot die zich vrijwillig onderwerpt aan de verplichte ziekte- en invaliditeitsverzekering – sector uitkeringen</xs:documentation>
					<xs:documentation xml:lang="fr">Conjoint aidant qui s'assujetti volontairement à l'assurance maladie et invalidité - secteur allocations</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="15">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Meewerkende echtgenoot die een vrijstelling van bijdrage heeft bekomen</xs:documentation>
					<xs:documentation xml:lang="fr">Conjoint aidant ayant obtenu une dispense de cotisations</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="16">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Zelfstandige in voortgezette verzekering wegens faillissement</xs:documentation>
					<xs:documentation xml:lang="fr">Indépendant en assurance continuée pour cause de faillite</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="17">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Gelijkstelling ziekte</xs:documentation>
					<xs:documentation xml:lang="fr">Assimilation maladie</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="18">
				<xs:annotation>
					<xs:documentation xml:lang="nl">Meewerkende echtgeno(o)t(e) maxi-statuut</xs:documentation>
					<xs:documentation xml:lang="fr">Conjoint(e) aidant(e) maxi-statut</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation xml:lang="nl">In geval van aanvraag tot vrijstelling van bijdragen</xs:documentation>
					<xs:documentation xml:lang="fr">Dans le cas d'une demande de dispense de cotisations</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
