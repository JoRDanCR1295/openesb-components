        </wsdl:operation>
    </portType>
    <binding name="SecureEndorsingCertificate_BasicPortBinding" type="tns:SecureEndorsingCertificate_Basic">
        <wsp:PolicyReference URI="#SecureEndorsingCertificate_BasicPortBindingPolicy"/>
        <wsdl:operation name="echo">
            <wsdl:input>
                <wsp:PolicyReference URI="#SecureEndorsingCertificate_BasicPortBinding_echo_Input_Policy"/>
            </wsdl:input>
            <wsdl:output>
                <wsp:PolicyReference URI="#SecureEndorsingCertificate_BasicPortBinding_echo_Output_Policy"/>
