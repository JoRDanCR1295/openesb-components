<?xml version="1.0" encoding="UTF-8"?>
<throttling>
    <config>
        <property name="rate" value="10 msg/sec" />
        <property name="queue-length" value="1000" />        
    </config>    
    <ruleset name="mythrottling">
        <rule name="myfirstrule">
            <xpath keyExpression="/Employee/FirstName"/>            
        </rule>
    </ruleset>
</throttling>