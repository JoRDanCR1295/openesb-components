<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="test_iep" xmlns:ns2="http://enterprise.netbeans.org/transformmap/xslt" version="1.0">
    <service-assembly>
        <identification>
            <name>jbi</name>
            <description>Represents the Service Assembly of jbi</description>
        </identification>
        <connections>
            <connection>
                <consumer endpoint-name="InputPort" service-name="ns1:InputService"/>
                <provider endpoint-name="InputRn" service-name="ns1:InputPl"/>
            </connection>
            <connection>
                <consumer endpoint-name="RequestReplyRn_InvokeService0" service-name="ns1:RequestReplyPl_InvokeService0"/>
                <provider endpoint-name="Service1" service-name="ns2:xsltse"/>
            </connection>
            <connection>
                <consumer endpoint-name="OutputRn_StreamOutput0" service-name="ns1:OutputPl_StreamOutput0"/>
                <provider endpoint-name="OutputPort_StreamOutput0" service-name="ns1:OutputService_StreamOutput0"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
