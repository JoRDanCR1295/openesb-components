    <file:binding/>
    <operation name="RelationOutput1">
        <file:operation/>
        <input>
            <file:message fileName="RelationOutput1.txt"
                    fileNameIsPattern="false"
                    addEOL="false"
                    multipleRecordsPerFile="true"
                    use="literal"/>
        </input>
    </operation>
</binding>
