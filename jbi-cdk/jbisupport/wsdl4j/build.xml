<project name="org.netbeans.modules.jbi.apisupport.wsdl4jlib" default="netbeans" basedir=".">
    <description>Builds, tests, and runs the project org.netbeans.modules.jbi.apisupport.wsdl4jlib.</description>
    <import file="nbproject/build-impl.xml"/>
    
    <target name="netbeans-extra" depends="init">
        <property name="release.dir" location="release"/>
        <mkdir dir="${release.dir}" />
        <copy file="../external/wsdl4j-1.6.2.jar" todir="${release.dir}/modules/ext" />
    </target>
    <target name="clean" depends="projectized-common.clean" >    
        <property name="release.dir" location="release"/>
        <echo message="cleaning release directory ${release.dir}" />
        <delete dir="${release.dir}" /> 
    </target>
</project>
