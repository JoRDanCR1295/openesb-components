<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
    "-//Puppy Crawl//DTD Suppressions 1.0//EN"
    "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">

<!--  Checkstyle suppression filter. See http://maven.apache.org/plugins/maven-checkstyle-plugin/customize.html.
	
	Used to avoid chestyle on sources automatically generated with Netbeans
	The generated sources are all in the it.imolinfo.jbi4ejb.component.runtime package
-->

<suppressions>
	<suppress checks="[\w]*" files="AbstractComponentLifeCycle" />

	<suppress checks="[\w]*" files="AbstractMessageExchangeHandler" />

	<suppress checks="[\w]*" files="ComponentInstaller" />

	<suppress checks="[\w]*" files="ComponentRuntime" />


	<suppress checks="[\w]*" files="DefaultComponentLifeCycle" />

	<suppress checks="[\w]*" files="DefaultMessageExchangeHandler" />


	<suppress checks="[\w]*" files="DefaultServiceUnitManager" />

	<suppress checks="[\w]*"
		files="InOutProviderMessageExchangeHandler" />

	<suppress checks="[\w]*" files="MessageExchangeHandler" />

	<suppress checks="[\w]*" files="MessageExchangeHandlerFactory" />


	<suppress checks="[\w]*" files="MessageExchangeReceiver" />

	<suppress checks="[\w]*" files="RuntimeContext" />

	<suppress checks="[\w]*" files="RuntimeHelper" />

	<suppress checks="[\w]*" files="ServiceDescriptor" />

</suppressions>



