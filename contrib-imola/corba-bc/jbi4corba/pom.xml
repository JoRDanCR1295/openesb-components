	        <exclude>it/imolinfo/jbi4corba/test/cxf/CXFProviderTest.java</exclude>
            <exclude>it/imolinfo/jbi4corba/test/runtime/ServiceCreatorTest.java</exclude>
            <!-- temporarily excluded to be reenabled -->
            <exclude>it/imolinfo/jbi4corba/test/bytecodemanipulation/AsmTestCase.java</exclude>
            <!-- <exclude>it/imolinfo/jbi4corba/test/webservice/generator/ConsumerServiceClassesGeneratorTest.java</exclude>
             -->
<!-- RUSST
