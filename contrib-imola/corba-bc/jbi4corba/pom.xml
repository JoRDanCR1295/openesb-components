  <build>
    <plugins>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-site-plugin</artifactId>
      </plugin>

      <plugin>
        <groupId>org.apache.servicemix.tooling</groupId>        
        <artifactId>jbi-maven-plugin</artifactId>
        <version>3.2.1</version>           
        <extensions>true</extensions>        
        <configuration>
          <type>binding-component</type>                 
          <bootstrapClassLoaderDelegation>self-first</bootstrapClassLoaderDelegation>
          <componentClassLoaderDelegation>self-first</componentClassLoaderDelegation>
          <bootstrap>it.imolinfo.jbi4corba.jbi.component.Jbi4CorbaInstaller</bootstrap>
          <component>it.imolinfo.jbi4corba.jbi.component.Jbi4CorbaRuntime</component>          
          <deployDependencies>false</deployDependencies>
          <generateJbiDescriptor>false</generateJbiDescriptor>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>        
        <configuration>
          <!-- To "false" to avoid the  "jaxb java.lang.LinkageError: loader constraints violated when linking" -->
          <childDelegation>false</childDelegation>
          <testFailureIgnore>false</testFailureIgnore>
          <parallel>false</parallel>
          <skip>false</skip>
          <forkMode>once</forkMode>
          <excludes>
            
            <!-- Excluded because parameter dir incorrected -->
            <exclude>it/imolinfo/jbi4corba/test/util/ClassCollectorTest.java</exclude>

            <exclude>it/imolinfo/jbi4corba/test/runtime/ConsumerServiceCreatorTest.java</exclude>
